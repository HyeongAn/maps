{
  "name": "maps",
  "private": true,
  "packageManager": "yarn@4.4.1",
  "scripts": {
    "dev:next": "yarn workspace next-app run dev",
    "build:next": "yarn workspace next-app run build",
    "dev:native": "yarn workspace webview run ios",
    "install:webview": "cd apps/webview && yarn install",
    "install:ios": "cd apps/webview/ios && pod install"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.25.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2"
  }
}
